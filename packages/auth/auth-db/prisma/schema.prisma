datasource pg {
  provider = "postgresql"
  url      = env("POSTGRES_URL")

}

generator photon {
  provider = "photonjs"
}

model Client {
  id    String  @default(cuid()) @id @unique
  email String  @unique
  username String  @unique
  password String
  firstName  String
  lastName  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}