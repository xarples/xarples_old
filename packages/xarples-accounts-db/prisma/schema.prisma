datasource pg {
  provider = "postgresql"
  url      = "postgresql://accounts:accounts@accounts-db/accounts"

}

generator photon {
  provider = "photonjs"
}

model User {
  id    String  @default(cuid()) @id @unique
  email String  @unique
  name  String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}